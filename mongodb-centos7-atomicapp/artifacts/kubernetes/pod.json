{
    "kind": "Pod",
    "apiVersion": "v1",
    "id": "mongodb",
    "metadata": {
        "name": "mongodb",
        "labels": {
            "name": "mongodb"
        }
    },
    "spec": {
        "containers": [
            {
                "name": "mongodb",
                "image": "$image",
                "ports": [
                    {
                        "containerPort": 27017
                    }
                ]
            }
        ]
    }
}
